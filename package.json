{
  "private": true,
  "name": "TODO",
  "scripts": {
    "build": "NODE_ENV=production vite build",
    "dev": "vite --open",
    "lint": "pnpm run lint:js:all -- --fix && pnpm run lint:css:all -- --fix",
    "lint:css": "stylelint --max-warnings 0 **/*.{css,vue}",
    "lint:css:all": "pnpm run lint:css -- **/*.{css,vue}",
    "lint:js": "eslint --max-warnings 0 --ext .js,.ts,.vue",
    "lint:js:all": "pnpm run lint:js -- .",
    "prepare": "husky install .husky",
    "tsc": "vue-tsc --noEmit",
    "serve": "vite preview"
  },
  "dependencies": {
    "@vueuse/core": "6.5.3",
    "pinia": "2.0.0-rc.12",
    "vue": "3.2.20",
    "vue-router": "4.0.11"
  },
  "devDependencies": {
    "@commitlint/cli": "13.2.1",
    "@commitlint/config-conventional": "13.2.0",
    "@iconify/json": "1.1.412",
    "@types/node": "16.9.6",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "@vitejs/plugin-vue": "1.9.3",
    "@vue/compiler-sfc": "3.2.20",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier-vue": "3.1.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-vue": "7.19.1",
    "husky": "7.0.2",
    "lint-staged": "11.2.2",
    "pnpm": "6.16.1",
    "prettier": "2.4.1",
    "stylelint": "13.13.1",
    "stylelint-config-idiomatic-order": "8.1.0",
    "stylelint-config-standard": "22.0.0",
    "typescript": "4.4.3",
    "unplugin-auto-import": "0.4.11",
    "unplugin-icons": "0.12.12",
    "unplugin-vue-components": "0.15.6",
    "vite": "2.6.5",
    "vite-plugin-pages": "0.18.1",
    "vite-plugin-pwa": "0.11.3",
    "vite-plugin-vue-layouts": "0.5.0",
    "vite-plugin-windicss": "1.4.10",
    "vue-tsc": "0.28.1",
    "windicss": "3.1.9",
    "workbox-core": "6.3.0",
    "workbox-precaching": "6.3.0",
    "workbox-window": "6.3.0"
  },
  "lint-staged": {
    "!(auto-imports.d.ts|components.d.ts)*.{js,ts,vue}": [
      "pnpm run lint:js -- --fix"
    ],
    "*.{css,vue}": [
      "pnpm run lint:css -- --fix"
    ]
  }
}
