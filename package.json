{
  "private": true,
  "name": "TODO",
  "scripts": {
    "build": "NODE_ENV=production vite build",
    "coverage": "vitest run --silent --coverage",
    "dev": "vite --open",
    "lint": "pnpm run lint:js:all --fix && pnpm run lint:css:all --fix",
    "lint:css": "stylelint --max-warnings 0 **/*.{css,vue}",
    "lint:css:all": "pnpm run lint:css **/*.{css,vue}",
    "lint:js": "eslint --max-warnings 0",
    "lint:js:all": "pnpm run lint:js .",
    "prepare": "husky install .husky",
    "serve": "vite preview",
    "test": "vitest run --silent",
    "test:watch": "vitest watch --silent",
    "tsc": "vue-tsc --noEmit && :"
  },
  "dependencies": {
    "@unocss/reset": "66.0.0",
    "@vueuse/core": "13.0.0",
    "daisyui": "3.9.4",
    "pinia": "3.0.1",
    "vue": "3.5.13",
    "vue-router": "4.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "19.8.0",
    "@commitlint/config-conventional": "19.8.0",
    "@iconify/json": "2.2.320",
    "@mdi/js": "7.4.47",
    "@types/node": "22.13.13",
    "@unocss/core": "66.0.0",
    "@unocss/preset-attributify": "66.0.0",
    "@unocss/preset-icons": "66.0.0",
    "@unocss/preset-mini": "66.0.0",
    "@unocss/preset-uno": "66.0.0",
    "@unocss/transformer-directives": "66.0.0",
    "@unocss/transformer-variant-group": "66.0.0",
    "@vitejs/plugin-vue": "5.2.3",
    "@vue/compiler-sfc": "3.5.13",
    "@vue/runtime-core": "3.5.13",
    "@vue/test-utils": "2.4.6",
    "eslint": "9.23.0",
    "eslint-config-prettier": "10.1.1",
    "eslint-plugin-prettier-vue": "5.0.0",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-vue": "10.0.0",
    "espree": "10.3.0",
    "husky": "9.1.7",
    "jsdom": "26.0.0",
    "lint-staged": "15.5.0",
    "pnpm": "9.15.9",
    "postcss-html": "1.8.0",
    "prettier": "3.5.3",
    "stylelint": "16.16.0",
    "stylelint-config-idiomatic-order": "10.0.0",
    "stylelint-config-standard": "37.0.0",
    "typescript": "5.8.2",
    "typescript-eslint": "8.28.0",
    "unocss": "66.0.0",
    "unocss-preset-daisy": "7.0.0",
    "unocss-preset-scrollbar": "3.2.0",
    "unplugin-auto-import": "19.1.1",
    "unplugin-vue-components": "28.4.1",
    "vite": "6.2.3",
    "vite-plugin-pages": "0.32.5",
    "vite-plugin-pwa": "0.21.2",
    "vite-plugin-vue-layouts": "0.11.0",
    "vitest": "3.0.9",
    "vue-tsc": "2.2.8",
    "workbox-core": "7.3.0",
    "workbox-precaching": "7.3.0",
    "workbox-window": "7.3.0"
  },
  "lint-staged": {
    "!(auto-imports.d.ts|components.d.ts)*.{js,ts,vue}": [
      "pnpm run lint:js --fix"
    ],
    "!(auto-imports.d.ts|components.d.ts)*.{js,ts,vue,hack}": [
      "pnpm run tsc"
    ],
    "*.{css,vue}": [
      "pnpm run lint:css --fix"
    ]
  },
  "packageManager": "pnpm@9.15.9",
  "prettier": {
    "singleAttributePerLine": true
  },
  "type": "module",
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "eslint": "9"
      }
    }
  }
}
