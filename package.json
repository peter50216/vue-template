{
  "private": true,
  "name": "TODO",
  "scripts": {
    "build": "NODE_ENV=production vite build",
    "dev": "vite --open",
    "lint": "pnpm run lint:js:all -- --fix && pnpm run lint:css:all -- --fix",
    "lint:css": "stylelint --max-warnings 0 **/*.{css,vue}",
    "lint:css:all": "pnpm run lint:css -- **/*.{css,vue}",
    "lint:js": "eslint --max-warnings 0 --ext .js,.ts,.vue",
    "lint:js:all": "pnpm run lint:js -- .",
    "prepare": "husky install .husky",
    "tsc": "vue-tsc --noEmit",
    "serve": "vite preview"
  },
  "dependencies": {
    "@vueuse/core": "8.2.1",
    "pinia": "2.0.12",
    "vue": "3.2.31",
    "vue-router": "4.0.14"
  },
  "devDependencies": {
    "@commitlint/cli": "16.2.3",
    "@commitlint/config-conventional": "16.2.1",
    "@iconify/json": "2.1.22",
    "@types/node": "16.11.26",
    "@typescript-eslint/eslint-plugin": "5.17.0",
    "@typescript-eslint/parser": "5.17.0",
    "@vitejs/plugin-vue": "2.3.1",
    "@vue/compiler-sfc": "3.2.31",
    "eslint": "8.12.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier-vue": "3.1.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-vue": "8.5.0",
    "husky": "7.0.4",
    "lint-staged": "12.3.7",
    "pnpm": "6.32.3",
    "postcss-html": "1.3.0",
    "prettier": "2.6.1",
    "stylelint": "14.6.1",
    "stylelint-config-idiomatic-order": "8.1.0",
    "stylelint-config-standard": "25.0.0",
    "typescript": "4.6.3",
    "unplugin-auto-import": "0.6.8",
    "unplugin-icons": "0.14.1",
    "unplugin-vue-components": "0.18.5",
    "vite": "2.9.0",
    "vite-plugin-pages": "0.22.0",
    "vite-plugin-pwa": "0.11.13",
    "vite-plugin-vue-layouts": "0.6.0",
    "vite-plugin-windicss": "1.8.3",
    "vue-tsc": "0.33.9",
    "windicss": "3.5.1",
    "workbox-core": "6.5.2",
    "workbox-precaching": "6.5.2",
    "workbox-window": "6.5.2"
  },
  "lint-staged": {
    "!(auto-imports.d.ts|components.d.ts)*.{js,ts,vue}": [
      "pnpm run lint:js -- --fix"
    ],
    "*.{css,vue}": [
      "pnpm run lint:css -- --fix"
    ]
  }
}
