{
  "private": true,
  "name": "TODO",
  "scripts": {
    "build": "NODE_ENV=production vite build",
    "coverage": "vitest run --silent --coverage",
    "dev": "vite --open",
    "lint": "pnpm run lint:js:all --fix && pnpm run lint:css:all --fix",
    "lint:css": "stylelint --max-warnings 0 **/*.{css,vue}",
    "lint:css:all": "pnpm run lint:css **/*.{css,vue}",
    "lint:js": "eslint --max-warnings 0 --ext .js,.ts,.vue",
    "lint:js:all": "pnpm run lint:js .",
    "prepare": "husky install .husky",
    "serve": "vite preview",
    "test": "vitest --silent",
    "tsc": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@unocss/reset": "0.33.4",
    "@vueuse/core": "8.4.2",
    "pinia": "2.0.14",
    "vue": "3.2.33",
    "vue-router": "4.0.15"
  },
  "devDependencies": {
    "@commitlint/cli": "16.2.4",
    "@commitlint/config-conventional": "16.2.4",
    "@iconify/json": "2.1.43",
    "@types/node": "16.11.35",
    "@typescript-eslint/eslint-plugin": "5.23.0",
    "@typescript-eslint/parser": "5.23.0",
    "@unocss/preset-attributify": "0.33.4",
    "@unocss/preset-icons": "0.33.4",
    "@unocss/preset-uno": "0.33.4",
    "@vitejs/plugin-vue": "2.3.3",
    "@vue/compiler-sfc": "3.2.33",
    "@vue/runtime-core": "3.2.33",
    "@vue/test-utils": "^2.0.0-rc.21",
    "eslint": "8.15.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier-vue": "3.1.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-vue": "8.7.1",
    "husky": "8.0.1",
    "jsdom": "^19.0.0",
    "lint-staged": "12.4.1",
    "pnpm": "7.1.0",
    "postcss-html": "1.4.1",
    "prettier": "2.6.2",
    "stylelint": "14.8.2",
    "stylelint-config-idiomatic-order": "8.1.0",
    "stylelint-config-standard": "25.0.0",
    "typescript": "4.6.4",
    "unocss": "0.33.4",
    "unocss-preset-scrollbar": "0.2.0",
    "unplugin-auto-import": "0.7.1",
    "unplugin-vue-components": "0.19.5",
    "vite": "2.9.9",
    "vite-plugin-pages": "0.23.0",
    "vite-plugin-pwa": "0.12.0",
    "vite-plugin-vue-layouts": "0.6.0",
    "vitest": "0.12.6",
    "vue-tsc": "0.34.13",
    "workbox-core": "6.5.3",
    "workbox-precaching": "6.5.3",
    "workbox-window": "6.5.3"
  },
  "lint-staged": {
    "!(auto-imports.d.ts|components.d.ts)*.{js,ts,vue}": [
      "pnpm run lint:js --fix"
    ],
    "*.{css,vue}": [
      "pnpm run lint:css --fix"
    ]
  },
  "packageManager": "pnpm@7.1.0",
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "unocss": "0"
      }
    }
  }
}
