{
  "private": true,
  "name": "TODO",
  "scripts": {
    "build": "NODE_ENV=production vite build",
    "dev": "vite --open",
    "lint": "pnpm run lint:js:all -- --fix && pnpm run lint:css:all -- --fix",
    "lint:css": "stylelint --max-warnings 0 **/*.{css,vue}",
    "lint:css:all": "pnpm run lint:css -- **/*.{css,vue}",
    "lint:js": "eslint --max-warnings 0 --ext .js,.ts,.vue",
    "lint:js:all": "pnpm run lint:js -- .",
    "prepare": "husky install .husky",
    "tsc": "vue-tsc --noEmit",
    "serve": "vite preview"
  },
  "dependencies": {
    "@vueuse/core": "7.4.1",
    "pinia": "2.0.9",
    "vue": "3.2.26",
    "vue-router": "4.0.12"
  },
  "devDependencies": {
    "@commitlint/cli": "15.0.0",
    "@commitlint/config-conventional": "15.0.0",
    "@iconify/json": "1.1.447",
    "@types/node": "16.11.17",
    "@typescript-eslint/eslint-plugin": "5.8.0",
    "@typescript-eslint/parser": "5.8.0",
    "@vitejs/plugin-vue": "2.0.1",
    "@vue/compiler-sfc": "3.2.26",
    "eslint": "8.5.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier-vue": "3.1.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-vue": "8.2.0",
    "husky": "7.0.4",
    "lint-staged": "12.1.4",
    "pnpm": "6.24.3",
    "postcss-html": "1.3.0",
    "prettier": "2.5.1",
    "stylelint": "14.2.0",
    "stylelint-config-idiomatic-order": "8.1.0",
    "stylelint-config-standard": "24.0.0",
    "typescript": "4.5.4",
    "unplugin-auto-import": "0.5.4",
    "unplugin-icons": "0.13.0",
    "unplugin-vue-components": "0.17.11",
    "vite": "2.7.6",
    "vite-plugin-pages": "0.19.7",
    "vite-plugin-pwa": "0.11.12",
    "vite-plugin-vue-layouts": "0.5.0",
    "vite-plugin-windicss": "1.6.1",
    "vue-tsc": "0.30.0",
    "windicss": "3.4.0",
    "workbox-core": "6.4.2",
    "workbox-precaching": "6.4.2",
    "workbox-window": "6.4.2"
  },
  "lint-staged": {
    "!(auto-imports.d.ts|components.d.ts)*.{js,ts,vue}": [
      "pnpm run lint:js -- --fix"
    ],
    "*.{css,vue}": [
      "pnpm run lint:css -- --fix"
    ]
  }
}
