{
  "private": true,
  "name": "TODO",
  "scripts": {
    "build": "NODE_ENV=production vite build",
    "coverage": "vitest run --silent --coverage",
    "dev": "vite --open",
    "lint": "pnpm run lint:js:all --fix && pnpm run lint:css:all --fix",
    "lint:css": "stylelint --max-warnings 0 **/*.{css,vue}",
    "lint:css:all": "pnpm run lint:css **/*.{css,vue}",
    "lint:js": "eslint --max-warnings 0 --ext .js,.ts,.vue",
    "lint:js:all": "pnpm run lint:js .",
    "prepare": "husky install .husky",
    "serve": "vite preview",
    "test": "vitest run --silent",
    "test:watch": "vitest watch --silent",
    "tsc": "vue-tsc --noEmit && :"
  },
  "dependencies": {
    "@unocss/reset": "0.57.1",
    "@vueuse/core": "10.5.0",
    "daisyui": "3.9.4",
    "pinia": "2.1.7",
    "vue": "3.3.7",
    "vue-router": "4.2.5"
  },
  "devDependencies": {
    "@commitlint/cli": "18.2.0",
    "@commitlint/config-conventional": "18.1.0",
    "@iconify/json": "2.2.135",
    "@mdi/js": "7.3.67",
    "@types/node": "20.8.10",
    "@typescript-eslint/eslint-plugin": "6.9.1",
    "@typescript-eslint/parser": "6.9.1",
    "@unocss/core": "0.57.1",
    "@unocss/preset-attributify": "0.57.1",
    "@unocss/preset-icons": "0.57.1",
    "@unocss/preset-mini": "0.57.1",
    "@unocss/preset-uno": "0.57.1",
    "@unocss/transformer-directives": "0.57.1",
    "@unocss/transformer-variant-group": "0.57.1",
    "@vitejs/plugin-vue": "4.4.0",
    "@vue-macros/reactivity-transform": "0.3.23",
    "@vue/compiler-sfc": "3.3.7",
    "@vue/runtime-core": "3.3.7",
    "@vue/test-utils": "2.4.1",
    "eslint": "8.52.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-prettier-vue": "5.0.0",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-vue": "9.18.1",
    "husky": "8.0.3",
    "jsdom": "22.1.0",
    "lint-staged": "15.0.2",
    "pnpm": "8.10.0",
    "postcss-html": "1.5.0",
    "prettier": "3.0.3",
    "stylelint": "15.11.0",
    "stylelint-config-idiomatic-order": "9.0.0",
    "stylelint-config-standard": "34.0.0",
    "typescript": "5.2.2",
    "unocss": "0.57.1",
    "unocss-preset-daisy": "7.0.0",
    "unocss-preset-scrollbar": "0.3.0",
    "unplugin-auto-import": "0.16.7",
    "unplugin-vue-components": "0.25.2",
    "vite": "4.5.0",
    "vite-plugin-pages": "0.31.0",
    "vite-plugin-pwa": "0.16.6",
    "vite-plugin-vue-layouts": "0.8.0",
    "vitest": "0.34.6",
    "vue-tsc": "1.8.22",
    "workbox-core": "7.0.0",
    "workbox-precaching": "7.0.0",
    "workbox-window": "7.0.0"
  },
  "lint-staged": {
    "!(auto-imports.d.ts|components.d.ts)*.{js,ts,vue}": [
      "pnpm run lint:js --fix"
    ],
    "!(auto-imports.d.ts|components.d.ts)*.{js,ts,vue,hack}": [
      "pnpm run tsc"
    ],
    "*.{css,vue}": [
      "pnpm run lint:css --fix"
    ]
