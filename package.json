{
  "private": true,
  "name": "TODO",
  "scripts": {
    "build": "NODE_ENV=production vite build",
    "dev": "vite --open",
    "lint": "pnpm run lint:js:all -- --fix && pnpm run lint:css:all -- --fix",
    "lint:css": "stylelint --max-warnings 0 **/*.{css,vue}",
    "lint:css:all": "pnpm run lint:css -- **/*.{css,vue}",
    "lint:js": "eslint --max-warnings 0 --ext .js,.ts,.vue",
    "lint:js:all": "pnpm run lint:js -- .",
    "prepare": "husky install .husky",
    "tsc": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@vueuse/core": "6.4.1",
    "pinia": "0.5.4",
    "vue": "3.2.18"
  },
  "devDependencies": {
    "@commitlint/cli": "13.1.0",
    "@commitlint/config-conventional": "13.1.0",
    "@iconify/json": "1.1.405",
    "@types/node": "16.9.6",
    "@typescript-eslint/eslint-plugin": "4.31.2",
    "@typescript-eslint/parser": "4.31.2",
    "@vitejs/plugin-vue": "1.9.2",
    "@vue/compiler-sfc": "3.2.18",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier-vue": "3.1.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-vue": "7.18.0",
    "husky": "7.0.2",
    "lint-staged": "11.1.2",
    "pnpm": "6.15.1",
    "prettier": "2.4.1",
    "stylelint": "13.13.1",
    "stylelint-config-idiomatic-order": "8.1.0",
    "stylelint-config-standard": "22.0.0",
    "typescript": "4.4.3",
    "unplugin-auto-import": "0.4.6",
    "unplugin-icons": "0.11.3",
    "unplugin-vue-components": "0.15.3",
    "vite": "2.5.10",
    "vite-plugin-windicss": "1.4.5",
    "vue-tsc": "0.3.0",
    "windicss": "3.1.7"
  },
  "lint-staged": {
    "!(auto-imports.d.ts)*.{js,ts,vue}": [
      "pnpm run lint:js -- --fix"
    ],
    "*.{css,vue}": [
      "pnpm run lint:css -- --fix"
    ]
  }
}
