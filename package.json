{
  "private": true,
  "name": "TODO",
  "scripts": {
    "build": "NODE_ENV=production vite build",
    "coverage": "vitest run --silent --coverage",
    "dev": "vite --open",
    "lint": "pnpm run lint:js:all --fix && pnpm run lint:css:all --fix",
    "lint:css": "stylelint --max-warnings 0 **/*.{css,vue}",
    "lint:css:all": "pnpm run lint:css **/*.{css,vue}",
    "lint:js": "eslint --max-warnings 0 --ext .js,.ts,.vue",
    "lint:js:all": "pnpm run lint:js .",
    "prepare": "husky install .husky",
    "serve": "vite preview",
    "test": "vitest --silent",
    "tsc": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@unocss/reset": "0.45.1",
    "@vueuse/core": "9.0.2",
    "pinia": "2.0.17",
    "vue": "3.2.37",
    "vue-router": "4.1.3"
  },
  "devDependencies": {
    "@commitlint/cli": "17.0.3",
    "@commitlint/config-conventional": "17.0.3",
    "@iconify/json": "2.1.86",
    "@types/node": "16.11.47",
    "@typescript-eslint/eslint-plugin": "5.32.0",
    "@typescript-eslint/parser": "5.32.0",
    "@unocss/preset-attributify": "0.45.1",
    "@unocss/preset-icons": "0.45.1",
    "@unocss/preset-uno": "0.45.1",
    "@vitejs/plugin-vue": "3.0.1",
    "@vue/compiler-sfc": "3.2.37",
    "@vue/runtime-core": "3.2.37",
    "@vue/test-utils": "2.0.2",
    "eslint": "8.21.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier-vue": "4.2.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-vue": "9.3.0",
    "husky": "8.0.1",
    "jsdom": "20.0.0",
    "lint-staged": "13.0.3",
    "pnpm": "7.8.0",
    "postcss-html": "1.5.0",
    "prettier": "2.7.1",
    "stylelint": "14.9.1",
    "stylelint-config-idiomatic-order": "8.1.0",
    "stylelint-config-standard": "26.0.0",
    "typescript": "4.7.4",
    "unocss": "0.45.1",
    "unocss-preset-scrollbar": "0.2.1",
    "unplugin-auto-import": "0.10.3",
    "unplugin-vue-components": "0.21.2",
    "vite": "3.0.4",
    "vite-plugin-pages": "0.25.0",
    "vite-plugin-pwa": "0.12.3",
    "vite-plugin-vue-layouts": "0.7.0",
    "vitest": "0.20.2",
    "vue-tsc": "0.39.4",
    "workbox-core": "6.5.4",
    "workbox-precaching": "6.5.4",
    "workbox-window": "6.5.4"
  },
  "lint-staged": {
    "!(auto-imports.d.ts|components.d.ts)*.{js,ts,vue}": [
      "pnpm run lint:js --fix"
    ],
    "*.{css,vue}": [
      "pnpm run lint:css --fix"
    ]
  },
  "packageManager": "pnpm@7.8.0",
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "vite": "3"
      }
    }
  }
}
