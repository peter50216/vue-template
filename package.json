{
  "private": true,
  "name": "TODO",
  "scripts": {
    "build": "NODE_ENV=production vite build",
    "coverage": "vitest run --silent --coverage",
    "dev": "vite --open",
    "lint": "pnpm run lint:js:all --fix && pnpm run lint:css:all --fix",
    "lint:css": "stylelint --max-warnings 0 **/*.{css,vue}",
    "lint:css:all": "pnpm run lint:css **/*.{css,vue}",
    "lint:js": "eslint --max-warnings 0",
    "lint:js:all": "pnpm run lint:js .",
    "prepare": "husky install .husky",
    "serve": "vite preview",
    "test": "vitest run --silent",
    "test:watch": "vitest watch --silent",
    "tsc": "vue-tsc --noEmit && :"
  },
  "dependencies": {
    "@unocss/reset": "0.59.0",
    "@vueuse/core": "10.9.0",
    "daisyui": "3.9.4",
    "pinia": "2.1.7",
    "vue": "3.4.21",
    "vue-router": "4.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "19.2.1",
    "@commitlint/config-conventional": "19.1.0",
    "@iconify/json": "2.2.198",
    "@mdi/js": "7.4.47",
    "@types/node": "20.12.5",
    "@unocss/core": "0.59.0",
    "@unocss/preset-attributify": "0.59.0",
    "@unocss/preset-icons": "0.59.0",
    "@unocss/preset-mini": "0.59.0",
    "@unocss/preset-uno": "0.59.0",
    "@unocss/transformer-directives": "0.59.0",
    "@unocss/transformer-variant-group": "0.59.0",
    "@vitejs/plugin-vue": "5.0.4",
    "@vue/compiler-sfc": "3.4.21",
    "@vue/runtime-core": "3.4.21",
    "@vue/test-utils": "2.4.5",
    "eslint": "9.0.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier-vue": "5.0.0",
    "eslint-plugin-simple-import-sort": "12.0.0",
    "eslint-plugin-vue": "9.24.1",
    "espree": "10.0.1",
    "husky": "9.0.11",
    "jsdom": "24.0.0",
    "lint-staged": "15.2.2",
    "pnpm": "8.15.6",
    "postcss-html": "1.6.0",
    "prettier": "3.2.5",
    "stylelint": "16.3.1",
    "stylelint-config-idiomatic-order": "10.0.0",
    "stylelint-config-standard": "36.0.0",
    "typescript": "5.4.4",
    "typescript-eslint": "7.5.0",
    "unocss": "0.59.0",
    "unocss-preset-daisy": "7.0.0",
    "unocss-preset-scrollbar": "0.3.1",
    "unplugin-auto-import": "0.17.5",
    "unplugin-vue-components": "0.26.0",
    "vite": "5.2.8",
    "vite-plugin-pages": "0.32.1",
    "vite-plugin-pwa": "0.19.8",
    "vite-plugin-vue-layouts": "0.11.0",
    "vitest": "1.4.0",
    "vue-tsc": "2.0.11",
    "workbox-core": "7.0.0",
    "workbox-precaching": "7.0.0",
    "workbox-window": "7.0.0"
  },
  "lint-staged": {
    "!(auto-imports.d.ts|components.d.ts)*.{js,ts,vue}": [
      "pnpm run lint:js --fix"
    ],
    "!(auto-imports.d.ts|components.d.ts)*.{js,ts,vue,hack}": [
      "pnpm run tsc"
    ],
    "*.{css,vue}": [
      "pnpm run lint:css --fix"
    ]
  },
  "packageManager": "pnpm@8.15.6",
  "prettier": {
    "singleAttributePerLine": true
  },
  "type": "module",
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "eslint": "9"
      }
    }
  }
}
