{
  "private": true,
  "name": "TODO",
  "scripts": {
    "build": "NODE_ENV=production vite build",
    "dev": "vite --open",
    "lint": "pnpm run lint:js:all -- --fix && pnpm run lint:css:all -- --fix",
    "lint:css": "stylelint --max-warnings 0 **/*.{css,vue}",
    "lint:css:all": "pnpm run lint:css -- **/*.{css,vue}",
    "lint:js": "eslint --max-warnings 0 --ext .js,.ts,.vue",
    "lint:js:all": "pnpm run lint:js -- .",
    "prepare": "husky install .husky",
    "tsc": "vue-tsc --noEmit",
    "serve": "vite preview"
  },
  "dependencies": {
    "@vueuse/core": "6.7.6",
    "pinia": "2.0.2",
    "vue": "3.2.21",
    "vue-router": "4.0.12"
  },
  "devDependencies": {
    "@commitlint/cli": "14.1.0",
    "@commitlint/config-conventional": "14.1.0",
    "@iconify/json": "1.1.423",
    "@types/node": "16.11.6",
    "@typescript-eslint/eslint-plugin": "5.3.0",
    "@typescript-eslint/parser": "5.3.0",
    "@vitejs/plugin-vue": "1.9.4",
    "@vue/compiler-sfc": "3.2.21",
    "eslint": "8.1.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier-vue": "3.1.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-vue": "8.0.3",
    "husky": "7.0.4",
    "lint-staged": "11.2.6",
    "pnpm": "6.20.3",
    "postcss-html": "1.2.0",
    "prettier": "2.4.1",
    "stylelint": "14.0.1",
    "stylelint-config-idiomatic-order": "8.1.0",
    "stylelint-config-standard": "23.0.0",
    "typescript": "4.4.4",
    "unplugin-auto-import": "0.4.13",
    "unplugin-icons": "0.12.18",
    "unplugin-vue-components": "0.17.0",
    "vite": "2.6.13",
    "vite-plugin-pages": "0.18.2",
    "vite-plugin-pwa": "0.11.3",
    "vite-plugin-vue-layouts": "0.5.0",
    "vite-plugin-windicss": "1.5.1",
    "vue-tsc": "0.28.10",
    "windicss": "3.2.1",
    "workbox-core": "6.3.0",
    "workbox-precaching": "6.3.0",
    "workbox-window": "6.3.0"
  },
  "lint-staged": {
    "!(auto-imports.d.ts|components.d.ts)*.{js,ts,vue}": [
      "pnpm run lint:js -- --fix"
    ],
    "*.{css,vue}": [
      "pnpm run lint:css -- --fix"
    ]
  }
}
