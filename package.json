{
  "private": true,
  "name": "TODO",
  "scripts": {
    "build": "NODE_ENV=production vite build",
    "dev": "vite --open",
    "lint": "pnpm run lint:js:all -- --fix && pnpm run lint:css:all -- --fix",
    "lint:css": "stylelint --max-warnings 0 **/*.{css,vue}",
    "lint:css:all": "pnpm run lint:css -- **/*.{css,vue}",
    "lint:js": "eslint --max-warnings 0 --ext .js,.ts,.vue",
    "lint:js:all": "pnpm run lint:js -- .",
    "prepare": "husky install .husky",
    "tsc": "vue-tsc --noEmit",
    "serve": "vite preview"
  },
  "dependencies": {
    "@vueuse/core": "7.7.1",
    "pinia": "2.0.11",
    "vue": "3.2.31",
    "vue-router": "4.0.13"
  },
  "devDependencies": {
    "@commitlint/cli": "16.2.1",
    "@commitlint/config-conventional": "16.2.1",
    "@iconify/json": "2.1.11",
    "@types/node": "16.11.26",
    "@typescript-eslint/eslint-plugin": "5.13.0",
    "@typescript-eslint/parser": "5.13.0",
    "@vitejs/plugin-vue": "2.2.4",
    "@vue/compiler-sfc": "3.2.31",
    "eslint": "8.10.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier-vue": "3.1.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-vue": "8.5.0",
    "husky": "7.0.4",
    "lint-staged": "12.3.4",
    "pnpm": "6.32.2",
    "postcss-html": "1.3.0",
    "prettier": "2.5.1",
    "stylelint": "14.5.3",
    "stylelint-config-idiomatic-order": "8.1.0",
    "stylelint-config-standard": "25.0.0",
    "typescript": "4.6.2",
    "unplugin-auto-import": "0.6.1",
    "unplugin-icons": "0.13.2",
    "unplugin-vue-components": "0.17.21",
    "vite": "2.8.6",
    "vite-plugin-pages": "0.21.3",
    "vite-plugin-pwa": "0.11.13",
    "vite-plugin-vue-layouts": "0.6.0",
    "vite-plugin-windicss": "1.8.2",
    "vue-tsc": "0.32.1",
    "windicss": "3.5.1",
    "workbox-core": "6.5.1",
    "workbox-precaching": "6.5.1",
    "workbox-window": "6.5.1"
  },
  "lint-staged": {
    "!(auto-imports.d.ts|components.d.ts)*.{js,ts,vue}": [
      "pnpm run lint:js -- --fix"
    ],
    "*.{css,vue}": [
      "pnpm run lint:css -- --fix"
    ]
  }
}
