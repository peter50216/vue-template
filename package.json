{
  "private": true,
  "name": "TODO",
  "scripts": {
    "build": "NODE_ENV=production vite build",
    "coverage": "vitest run --silent --coverage",
    "dev": "vite --open",
    "lint": "pnpm run lint:js:all --fix && pnpm run lint:css:all --fix",
    "lint:css": "stylelint --max-warnings 0 **/*.{css,vue}",
    "lint:css:all": "pnpm run lint:css **/*.{css,vue}",
    "lint:js": "eslint --max-warnings 0",
    "lint:js:all": "pnpm run lint:js .",
    "prepare": "husky install .husky",
    "serve": "vite preview",
    "test": "vitest run --silent",
    "test:watch": "vitest watch --silent",
    "tsc": "vue-tsc --noEmit && :"
  },
  "dependencies": {
    "@unocss/reset": "66.5.1",
    "@vueuse/core": "13.9.0",
    "daisyui": "3.9.4",
    "pinia": "3.0.3",
    "vue": "3.5.21",
    "vue-router": "4.5.1"
  },
  "devDependencies": {
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@iconify/json": "2.2.382",
    "@mdi/js": "7.4.47",
    "@types/node": "22.18.1",
    "@unocss/core": "66.5.1",
    "@unocss/preset-attributify": "66.5.1",
    "@unocss/preset-icons": "66.5.1",
    "@unocss/preset-mini": "66.5.1",
    "@unocss/preset-uno": "66.5.1",
    "@unocss/transformer-directives": "66.5.1",
    "@unocss/transformer-variant-group": "66.5.1",
    "@vitejs/plugin-vue": "5.2.4",
    "@vue/compiler-sfc": "3.5.21",
    "@vue/runtime-core": "3.5.21",
    "@vue/test-utils": "2.4.6",
    "eslint": "9.35.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier-vue": "5.0.0",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-vue": "10.4.0",
    "espree": "10.4.0",
    "husky": "9.1.7",
    "jsdom": "26.1.0",
    "lint-staged": "16.1.6",
    "pnpm": "9.15.9",
    "postcss-html": "1.8.0",
    "prettier": "3.6.2",
    "stylelint": "16.24.0",
    "stylelint-config-idiomatic-order": "10.0.0",
    "stylelint-config-standard": "39.0.0",
    "typescript": "5.9.2",
    "typescript-eslint": "8.42.0",
    "unocss": "66.5.1",
    "unocss-preset-daisy": "7.0.0",
    "unocss-preset-scrollbar": "3.2.0",
    "unplugin-auto-import": "20.1.0",
    "unplugin-vue-components": "29.0.0",
    "vite": "6.3.5",
    "vite-plugin-pages": "0.33.1",
    "vite-plugin-pwa": "0.21.2",
    "vite-plugin-vue-layouts": "0.11.0",
    "vitest": "3.2.4",
    "vue-tsc": "3.0.6",
    "workbox-core": "7.3.0",
    "workbox-precaching": "7.3.0",
    "workbox-window": "7.3.0"
  },
  "lint-staged": {
    "!(auto-imports.d.ts|components.d.ts)*.{js,ts,vue}": [
      "pnpm run lint:js --fix"
    ],
    "!(auto-imports.d.ts|components.d.ts)*.{js,ts,vue,hack}": [
      "pnpm run tsc"
    ],
    "*.{css,vue}": [
      "pnpm run lint:css --fix"
    ]
  },
  "packageManager": "pnpm@9.15.9",
  "prettier": {
    "singleAttributePerLine": true
  },
  "type": "module",
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "eslint": "9"
      }
    }
  }
