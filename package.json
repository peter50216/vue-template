{
  "private": true,
  "name": "TODO",
  "scripts": {
    "build": "NODE_ENV=production vite build",
    "coverage": "vitest run --silent --coverage",
    "dev": "vite --open",
    "lint": "pnpm run lint:js:all --fix && pnpm run lint:css:all --fix",
    "lint:css": "stylelint --max-warnings 0 **/*.{css,vue}",
    "lint:css:all": "pnpm run lint:css **/*.{css,vue}",
    "lint:js": "eslint --max-warnings 0 --ext .js,.ts,.vue",
    "lint:js:all": "pnpm run lint:js .",
    "prepare": "husky install .husky",
    "serve": "vite preview",
    "test": "vitest run --silent",
    "test:watch": "vitest watch --silent",
    "tsc": "vue-tsc --noEmit && :"
  },
  "dependencies": {
    "@unocss/reset": "0.58.5",
    "@vueuse/core": "10.9.0",
    "daisyui": "3.9.4",
    "pinia": "2.1.7",
    "vue": "3.4.21",
    "vue-router": "4.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "19.0.3",
    "@commitlint/config-conventional": "19.0.3",
    "@iconify/json": "2.2.188",
    "@mdi/js": "7.4.47",
    "@types/node": "20.11.24",
    "@typescript-eslint/eslint-plugin": "7.1.0",
    "@typescript-eslint/parser": "7.1.0",
    "@unocss/core": "0.58.5",
    "@unocss/preset-attributify": "0.58.5",
    "@unocss/preset-icons": "0.58.5",
    "@unocss/preset-mini": "0.58.5",
    "@unocss/preset-uno": "0.58.5",
    "@unocss/transformer-directives": "0.58.5",
    "@unocss/transformer-variant-group": "0.58.5",
    "@vitejs/plugin-vue": "5.0.4",
    "@vue-macros/reactivity-transform": "0.4.3",
    "@vue/compiler-sfc": "3.4.21",
    "@vue/runtime-core": "3.4.21",
    "@vue/test-utils": "2.4.4",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier-vue": "5.0.0",
    "eslint-plugin-simple-import-sort": "12.0.0",
    "eslint-plugin-vue": "9.22.0",
    "husky": "9.0.11",
    "jsdom": "24.0.0",
    "lint-staged": "15.2.2",
    "pnpm": "8.15.4",
    "postcss-html": "1.6.0",
    "prettier": "3.2.5",
    "stylelint": "16.2.1",
    "stylelint-config-idiomatic-order": "10.0.0",
    "stylelint-config-standard": "36.0.0",
    "typescript": "5.3.3",
    "unocss": "0.58.5",
    "unocss-preset-daisy": "7.0.0",
    "unocss-preset-scrollbar": "0.3.1",
    "unplugin-auto-import": "0.17.5",
    "unplugin-vue-components": "0.26.0",
    "vite": "5.1.4",
    "vite-plugin-pages": "0.32.0",
    "vite-plugin-pwa": "0.19.2",
    "vite-plugin-vue-layouts": "0.11.0",
    "vitest": "1.3.1",
    "vue-tsc": "2.0.2",
    "workbox-core": "7.0.0",
    "workbox-precaching": "7.0.0",
    "workbox-window": "7.0.0"
  },
  "lint-staged": {
    "!(auto-imports.d.ts|components.d.ts)*.{js,ts,vue}": [
      "pnpm run lint:js --fix"
    ],
    "!(auto-imports.d.ts|components.d.ts)*.{js,ts,vue,hack}": [
      "pnpm run tsc"
    ],
    "*.{css,vue}": [
      "pnpm run lint:css --fix"
