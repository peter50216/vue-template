{
  "private": true,
  "name": "TODO",
  "scripts": {
    "build": "NODE_ENV=production vite build",
    "dev": "vite --open",
    "lint": "pnpm run lint:js:all -- --fix && pnpm run lint:css:all -- --fix",
    "lint:css": "stylelint --max-warnings 0 **/*.{css,vue}",
    "lint:css:all": "pnpm run lint:css -- **/*.{css,vue}",
    "lint:js": "eslint --max-warnings 0 --ext .js,.ts,.vue",
    "lint:js:all": "pnpm run lint:js -- .",
    "prepare": "husky install .husky",
    "tsc": "vue-tsc --noEmit",
    "serve": "vite preview"
  },
  "dependencies": {
    "@unocss/reset": "0.31.5",
    "@vueuse/core": "8.2.6",
    "pinia": "2.0.13",
    "vue": "3.2.33",
    "vue-router": "4.0.14"
  },
  "devDependencies": {
    "@commitlint/cli": "16.2.3",
    "@commitlint/config-conventional": "16.2.1",
    "@iconify/json": "2.1.29",
    "@types/node": "16.11.27",
    "@typescript-eslint/eslint-plugin": "5.20.0",
    "@typescript-eslint/parser": "5.20.0",
    "@unocss/preset-attributify": "0.31.5",
    "@unocss/preset-icons": "0.31.5",
    "@unocss/preset-uno": "0.31.5",
    "@vitejs/plugin-vue": "2.3.1",
    "@vue/compiler-sfc": "3.2.33",
    "eslint": "8.13.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier-vue": "3.1.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-vue": "8.6.0",
    "husky": "7.0.4",
    "lint-staged": "12.3.8",
    "pnpm": "6.32.8",
    "postcss-html": "1.4.1",
    "prettier": "2.6.2",
    "stylelint": "14.7.1",
    "stylelint-config-idiomatic-order": "8.1.0",
    "stylelint-config-standard": "25.0.0",
    "typescript": "4.6.3",
    "unocss": "0.31.5",
    "unocss-preset-scrollbar": "0.0.1",
    "unplugin-auto-import": "0.7.1",
    "unplugin-vue-components": "0.19.3",
    "vite": "2.9.5",
    "vite-plugin-pages": "0.23.0",
    "vite-plugin-pwa": "0.11.13",
    "vite-plugin-vue-layouts": "0.6.0",
    "vue-tsc": "0.34.7",
    "workbox-core": "6.5.3",
    "workbox-precaching": "6.5.3",
    "workbox-window": "6.5.3"
  },
  "lint-staged": {
    "!(auto-imports.d.ts|components.d.ts)*.{js,ts,vue}": [
      "pnpm run lint:js -- --fix"
    ],
    "*.{css,vue}": [
      "pnpm run lint:css -- --fix"
    ]
  }
}
